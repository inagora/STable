{
	"name": "@inagora/stable",
	"version": "2.0.18",
	"description": "STable是一个以表格展示和操作为核心功能的工具。",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --mode=development --usehmr --config=./build/webpack.config.js",
		"dev": "webpack --mode=development --config=./build/webpack.config.js",
		"prod": "webpack --mode=production --no-progress --hide-modules --config=./build/webpack.config.js; node ./build/csscompress.js",
		"prod2": "webpack --mode=production --no-progress --hide-modules --config=./build/webpack.config.js",
		"lang": "webpack --mode=production --no-progress --hide-modules --config=./build/webpack-lang.config.js",
		"docs:build": "vuepress build docs",
		"docs:dev": "vuepress dev docs"
	},
	"author": "lijieniu, poker",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/parser": "^7.9.4",
		"@babel/preset-env": "^7.9.0",
		"babel-loader": "^8.1.0",
		"babel-minify-webpack-plugin": "^0.3.1",
		"css-loader": "^3.4.2",
		"eslint": "^6.8.0",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-vue": "^6.2.2",
		"mini-css-extract-plugin": "^0.9.0",
		"multer": "^1.4.2",
		"node-sass": "^4.13.1",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3",
		"vue": "^2.6.11",
		"vue-loader": "^15.9.1",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.6.11",
		"vuepress": "^1.4.0",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	},
	"directories": {
		"doc": "docs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/inagora/STable.git"
	},
	"keywords": [
		"table",
		"vue",
		"javascript"
	],
	"bugs": {
		"url": "https://github.com/inagora/STable/issues"
	},
	"homepage": "https://github.com/inagora/STable#readme",
	"eslintConfig": {
		"root": true,
		"env": {
			"browser": true,
			"es2021": true
		},
		"globals": {
			"WdView": true,
			"XLSX": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:vue/essential",
			"plugin:@typescript-eslint/recommended",
			"@wd/eslint-config-wd",
			"prettier"
		],
		"parser": "vue-eslint-parser",
		"parserOptions": {
			"ecmaVersion": 12,
			"parser": "@typescript-eslint/parser",
			"sourceType": "module"
		},
		"plugins": [
			"vue",
			"@typescript-eslint"
		],
		"rules": {}
	},
	"prettier": {
		"tabWidth": 2,
		"printWidth": 80,
		"useTabs": true,
		"semi": true,
		"singleQuote": true
	}
}
